{
    "name": "quickapp-for-highlighter",
    "displayName": "QuickApp For Highlighter",
    "description": "QuickApp for Highlighter",
    "version": "1.0.3",
    "publisher": "Atom",
    "icon": "img/logo.png",
    "engines": {
        "vscode": "^0.10.5"
    },
    "keywords": [
        "quickapp",
        "ux",
        "Hap",
        "Highlighter"
    ],
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ux",
            "aliases": ["ux", "ux"],
            "extensions": [".ux"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ux",
            "scopeName": "text.html.ux",
            "path": "./syntaxes/ux.tmLanguage"
        }],
        "snippets": [
            {
             "language": "javascript",
             "path": "./snippets/javascript.json"
            },
            {
             "language": "typescript",
             "path": "./snippets/javascript.json"
            },
            {
             "language": "ux",
             "path": "./snippets/javascript.json"
            },
            {
             "language": "ux",
             "path": "./snippets/html.json"
            },
            {
             "language": "ux",
             "path": "./snippets/ux.json"
            }
        ]
    }
}